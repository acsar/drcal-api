# =====================================================
# SUPABASE CONFIGURATION (OBRIGATÓRIO)
# =====================================================
SUPABASE_URL=sua_url_do_supabase
SUPABASE_KEY=sua_chave_anon_do_supabase

# =====================================================
# REDIS CONFIGURATION (OBRIGATÓRIO)
# =====================================================
# Host do Redis (padrão: localhost)
# Em Docker: use o nome do serviço (ex: redis)
REDIS_HOST=localhost

# Porta do Redis (padrão: 6379)
REDIS_PORT=6379

# Redis configurado sem autenticação para ambiente Docker
# REDIS_PASSWORD não é necessário

# =====================================================
# SERVER CONFIGURATION
# =====================================================
PORT=3000
NODE_ENV=development